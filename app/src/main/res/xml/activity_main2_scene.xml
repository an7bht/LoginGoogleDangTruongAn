<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <KeyFrameSet>
        </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/img_rotage"
            motion:dragDirection="dragDown" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/img_rotage_min"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            motion:layout_constraintHorizontal_bias="0.032"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="496dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/view2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="412dp"
            android:layout_height="153dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="476dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="340dp"
            android:layout_height="24dp"
            motion:layout_constraintTop_toBottomOf="@+id/textview_animation"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0"
            motion:layout_editor_absoluteX="26dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            android:layout_width="wrap_content"
            motion:layout_constraintVertical_bias="0.533"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginStart="36dp"
            android:id="@+id/textview_animation"
            android:layout_marginLeft="36dp" />
        <Constraint
            android:layout_height="10dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="20dp"
            motion:layout_constraintBottom_toTopOf="@+id/imageView8"
            motion:layout_constraintHorizontal_bias="0.459"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="320dp"
            android:id="@+id/progressBarBB"
            motion:layout_editor_absoluteY="490dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/textView3"
            motion:layout_constraintHorizontal_bias="0.906"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/txtTotaltime" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="63dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="520dp"
            motion:layout_constraintHorizontal_bias="0.058"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="72dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:id="@+id/img_rotage"
            android:alpha="0" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            motion:layout_editor_absoluteX="101dp"
            motion:layout_constraintTop_toTopOf="@+id/img_rotage_min"
            motion:layout_constraintBottom_toBottomOf="@+id/img_rotage"
            motion:layout_constraintVertical_bias="0.0" >
            <CustomAttribute
                motion:attributeName="textColor" />
        </Constraint>
        <Constraint
            android:layout_height="18dp"
            motion:layout_constraintHorizontal_bias="0.278"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView6"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="40dp"
            motion:layout_constraintHorizontal_bias="0.859"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imgStart"
            motion:layout_constraintTop_toBottomOf="@+id/imgStop0"
            android:layout_marginTop="12dp" />
        <Constraint
            android:id="@+id/imageView9"
            motion:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_width="178dp"
            android:layout_height="63dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView8"
            motion:layout_constraintTop_toTopOf="@+id/imageView8"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imageView8"
            motion:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_width="23dp"
            android:layout_height="23dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView6"
            motion:layout_constraintTop_toTopOf="@+id/imageView6"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imgStarttttt"
            motion:layout_constraintStart_toEndOf="@+id/imgStart"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imgStart"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="44dp"
            motion:layout_constraintTop_toTopOf="@+id/imgStart"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imgTua"
            motion:layout_constraintEnd_toStartOf="@+id/imgStart"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imgStart"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginEnd="44dp"
            motion:layout_constraintTop_toTopOf="@+id/imgStart"
            android:alpha="0" />
        <Constraint
            android:layout_height="159dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="496dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="413dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:id="@+id/view2" />
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintStart_toEndOf="@+id/imgStarttttt"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imgStarttttt"
            motion:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="28dp"
            motion:layout_constraintTop_toTopOf="@+id/imgStarttttt"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toStartOf="@+id/imgTua"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imgTua"
            motion:layout_constraintVertical_bias="1.0"
            android:layout_marginEnd="20dp"
            motion:layout_constraintTop_toTopOf="@+id/imgTua"
            android:alpha="0" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            android:layout_width="wrap_content"
            motion:layout_constraintVertical_bias="0.533"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginStart="36dp"
            android:id="@+id/textview_animation"
            android:layout_marginLeft="36dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="520dp"
            motion:layout_constraintHorizontal_bias="0.058"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:id="@+id/img_rotage_min" />
        <Constraint
            android:layout_height="9dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/imageView8"
            motion:layout_constraintHorizontal_bias="0.53"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="215dp"
            android:id="@+id/progressBarBB"
            android:layout_marginBottom="96dp" />
        <Constraint
            android:id="@+id/txtProcess"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/textView3"
            motion:layout_constraintHorizontal_bias="0.076"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
        <Constraint
            android:id="@+id/txtTotaltime"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/textView3"
            motion:layout_constraintHorizontal_bias="0.827"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
    </ConstraintSet>
</MotionScene>